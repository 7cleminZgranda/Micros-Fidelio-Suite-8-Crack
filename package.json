{
  "name": "ember-power-datepicker",
  "version": "0.9.0",
  "description": "An extensible datepicker for Ember",
  "keywords": [
    "ember-addon",
    "ember",
    "component",
    "power",
    "datepicker",
    "calendar",
    "dropdown"
  ],
  "repository": "https://github.com/cibernox/ember-power-datepicker",
  "license": "MIT",
  "author": "Miguel Camba",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\" --allow-empty-input",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:scss": "stylelint \"**/*.scss\" --allow-empty-input",
    "lint:scss:fix": "concurrently \"npm:lint:scss -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember server",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "ember-auto-import": "^2.6.3",
    "ember-basic-dropdown": "^7.2.1",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-power-calendar": "git+https://git@github.com/cibernox/ember-power-calendar.git#ed1b35041403a2b522a1a06cc2b5e71a9261656d"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.10",
    "@babel/plugin-proposal-decorators": "^7.22.10",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^3.2.0",
    "@embroider/test-setup": "^3.0.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "bootstrap-sass": "^3.4.3",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.2.0",
    "ember-assign-helper": "^0.4.0",
    "ember-cli": "~4.12.0",
    "ember-cli-app-version": "^6.0.1",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^8.0.0",
    "ember-power-calendar-moment": "git+https://git@github.com/mkszepp/ember-power-calendar-moment.git#9497cf4ff7d9cf94f74d9c091daca3f04eaaf4d6",
    "ember-qunit": "^7.0.0",
    "ember-resolver": "^11.0.1",
    "ember-source": "~4.12.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.11.2",
    "ember-truth-helpers": "^3.1.1",
    "ember-try": "^3.0.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-ember": "^11.10.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-qunit": "^8.0.0",
    "loader.js": "^4.7.0",
    "moment": "^2.29.4",
    "prettier": "^3.0.2",
    "qunit": "^2.19.4",
    "qunit-dom": "^2.0.0",
    "sass": "^1.66.0",
    "stylelint": "^15.10.2",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-prettier": "^4.0.2",
    "webpack": "^5.88.2"
  },
  "peerDependencies": {
    "ember-source": "^3.28.0 || ^4.0.0 || >=5.0.0"
  },
  "engines": {
    "node": "16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
